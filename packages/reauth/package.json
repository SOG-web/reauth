{
  "name": "@re-auth/reauth",
  "version": "0.1.0-alpha.28",
  "description": "Runtime, framework, and protocol-independent authentication engine for TypeScript/JavaScript applications with plugin-based architecture",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./plugins/anonymous": {
      "types": "./dist/plugins/anonymous/index.d.ts",
      "import": "./dist/plugins/anonymous/index.js",
      "require": "./dist/plugins/anonymous/index.cjs"
    },
    "./plugins/email-password": {
      "types": "./dist/plugins/email-password/index.d.ts",
      "import": "./dist/plugins/email-password/index.js",
      "require": "./dist/plugins/email-password/index.cjs"
    },
    "./plugins/email-or-username": {
      "types": "./dist/plugins/email-or-username/index.d.ts",
      "import": "./dist/plugins/email-or-username/index.js",
      "require": "./dist/plugins/email-or-username/index.cjs"
    },
    "./plugins/jwt": {
      "types": "./dist/plugins/jwt/index.d.ts",
      "import": "./dist/plugins/jwt/index.js",
      "require": "./dist/plugins/jwt/index.cjs"
    },
    "./plugins/phone": {
      "types": "./dist/plugins/phone/index.d.ts",
      "import": "./dist/plugins/phone/index.js",
      "require": "./dist/plugins/phone/index.cjs"
    },
    "./plugins/api-key": {
      "types": "./dist/plugins/api-key/index.d.ts",
      "import": "./dist/plugins/api-key/index.js",
      "require": "./dist/plugins/api-key/index.cjs"
    },
    "./plugins/organization": {
      "types": "./dist/plugins/organization/index.d.ts",
      "import": "./dist/plugins/organization/index.js",
      "require": "./dist/plugins/organization/index.cjs"
    },
    "./plugins/passwordless": {
      "types": "./dist/plugins/passwordless/index.d.ts",
      "import": "./dist/plugins/passwordless/index.js",
      "require": "./dist/plugins/passwordless/index.cjs"
    },
    "./plugins/session": {
      "types": "./dist/plugins/session/index.d.ts",
      "import": "./dist/plugins/session/index.js",
      "require": "./dist/plugins/session/index.cjs"
    },
    "./plugins/username": {
      "types": "./dist/plugins/username/index.d.ts",
      "import": "./dist/plugins/username/index.js",
      "require": "./dist/plugins/username/index.cjs"
    },
    "./plugins/admin": {
      "types": "./dist/plugins/admin/index.d.ts",
      "import": "./dist/plugins/admin/index.js",
      "require": "./dist/plugins/admin/index.cjs"
    },
    "./services": {
      "types": "./dist/services/index.d.ts",
      "import": "./dist/services/index.js",
      "require": "./dist/services/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "plugins/anonymous": [
        "./dist/plugins/anonymous/index.d.ts"
      ],
      "plugins/email-password": [
        "./dist/plugins/email-password/index.d.ts"
      ],
      "plugins/email-or-username": [
        "./dist/plugins/email-or-username/index.d.ts"
      ],
      "plugins/jwt": [
        "./dist/plugins/jwt/index.d.ts"
      ],
      "plugins/phone": [
        "./dist/plugins/phone/index.d.ts"
      ],
      "plugins/api-key": [
        "./dist/plugins/api-key/index.d.ts"
      ],
      "plugins/organization": [
        "./dist/plugins/organization/index.d.ts"
      ],
      "plugins/passwordless": [
        "./dist/plugins/passwordless/index.d.ts"
      ],
      "plugins/session": [
        "./dist/plugins/session/index.d.ts"
      ],
      "plugins/username": [
        "./dist/plugins/username/index.d.ts"
      ],
      "plugins/admin": [
        "./dist/plugins/admin/index.d.ts"
      ],
      "services": [
        "./dist/services/index.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm clean && tsup",
    "build:esm": "tsup --format esm --outdir dist/esm",
    "build:cjs": "tsup --format cjs --outdir dist/cjs",
    "dev": "pnpm build:esm --watch",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint:package": "publint"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SOG-web/reauth.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/deno": "^2.2.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.21",
    "publint": "^0.3.13",
    "tslib": "^2.8.1",
    "tsup": "^8.5.0",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@node-rs/argon2": "^2.0.2",
    "@oslojs/crypto": "^1.0.1",
    "@standard-schema/spec": "^1.0.0",
    "arctic": "^3.6.1",
    "arktype": "^2.1.20",
    "awilix": "^12.0.5",
    "axios": "^1.9.0",
    "fumadb": "^0.1.0",
    "joi": "^18.0.1",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "qrcode": "^1.5.4",
    "rfc4648": "^1.5.4",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
  }
}
