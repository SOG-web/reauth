{
  "name": "@re-auth/http-adapters",
  "version": "1.0.0-alpha.18",
  "description": "V2 HTTP protocol adapters for ReAuth - framework-agnostic integration with Express, Fastify, and Hono",
  "type": "module",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/esm/src/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/esm/src/index.d.ts",
      "import": "./dist/esm/src/index.js",
      "require": "./dist/cjs/src/index.js"
    },
    "./adapters/*": {
      "types": "./dist/esm/src/adapters/*.d.ts",
      "import": "./dist/esm/src/adapters/*.js",
      "require": "./dist/cjs/src/adapters/*.js"
    },
    "./middleware/*": {
      "types": "./dist/esm/src/middleware/*.d.ts",
      "import": "./dist/esm/src/middleware/*.js",
      "require": "./dist/cjs/src/middleware/*.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/esm/src/index.d.ts"
      ],
      "adapters/*": [
        "./dist/esm/src/adapters/*.d.ts"
      ],
      "middleware/*": [
        "./dist/esm/src/middleware/*.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "pnpm clean && pnpm build:esm && pnpm build:cjs",
    "dev": "pnpm build:esm --watch",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@better-fetch/fetch": "^1.1.18",
    "@re-auth/reauth": "workspace:*",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "itty-router": "^5.0.22",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.2",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "express": "^5.1.0",
    "fastify": "^5.3.3",
    "hono": "^4.7.10",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@re-auth/reauth": "workspace:*"
  },
  "optionalDependencies": {
    "express": "^5.1.0",
    "fastify": "^5.3.3",
    "hono": "^4.7.10"
  },
  "keywords": [
    "re-auth",
    "http",
    "adapters",
    "authentication",
    "middleware",
    "v2",
    "express",
    "fastify",
    "hono"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SOG-web/reauth"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
