{
  "name": "@re-auth/http-adapters",
  "version": "1.0.0-alpha.32",
  "description": "HTTP protocol adapters for ReAuth - framework-agnostic integration with Express, Fastify, and Hono",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./base-adapter": {
      "types": "./dist/base-adapter.d.ts",
      "import": "./dist/base-adapter.js",
      "require": "./dist/base-adapter.cjs"
    },
    "./adapters/*": {
      "types": "./dist/adapters/*.d.ts",
      "import": "./dist/adapters/*.js",
      "require": "./dist/adapters/*.cjs"
    },
    "./middleware/*": {
      "types": "./dist/middleware/*.d.ts",
      "import": "./dist/middleware/*.js",
      "require": "./dist/middleware/*.cjs"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.js",
      "require": "./dist/utils/*.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "base-adapter": [
        "./dist/base-adapter.d.ts"
      ],
      "adapters/*": [
        "./dist/adapters/*.d.ts"
      ],
      "middleware/*": [
        "./dist/middleware/*.d.ts"
      ],
      "utils/*": [
        "./dist/utils/*.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@better-fetch/fetch": "^1.1.18",
    "@re-auth/reauth": "workspace:*",
    "cors": "^2.8.5",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "itty-router": "^5.0.22",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.2",
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "express": "^5.1.0",
    "fastify": "^5.3.3",
    "hono": "^4.7.10",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@re-auth/reauth": "workspace:*"
  },
  "optionalDependencies": {
    "express": "^5.1.0",
    "fastify": "^5.3.3",
    "hono": "^4.7.10"
  },
  "keywords": [
    "re-auth",
    "http",
    "adapters",
    "authentication",
    "middleware",
    "v2",
    "express",
    "fastify",
    "hono"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SOG-web/reauth"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
