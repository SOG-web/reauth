{
  "name": "hono-test",
  "description": "Example HTTP protocol integration demonstrating ReAuth with Hono framework using HTTP adapters",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "tsx ./src/reauth/cli.ts migrate:up",
    "migrate:down": "tsx ./src/reauth/cli.ts migrate:down",
    "migrate:status": "tsx ./src/reauth/cli.ts migrate:status"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.2",
    "@re-auth/http-adapters": "workspace:*",
    "@re-auth/reauth": "workspace:*",
    "@re-auth/logger": "workspace:*",
    "better-sqlite3": "^11.10.0",
    "fumadb": "^0.1.0",
    "hono": "^4.7.10",
    "kysely": "^0.28.7"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.11.17",
    "esbuild": "^0.25.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.8.3"
  }
}
